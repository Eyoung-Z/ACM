#include<bits/stdc++.h>
#define N 10005
using namespace std;
int n;
vector<int>v;
int main()
{
	int r; 
	scanf("%d",&n);
	for(int i=0;i<n,n>0;i++)
	{
		r=n%26;
		if(r==0) //
		r=26;
		v.push_back(r);//
		n=(n-r)/26;
	}
	for(int i=v.size()-1;i>=0;i--)
	{
		if(v[i]<1)a
        printf("%c",'A'+v[i]+25);
        else
		printf("%c",'A'+v[i]-1);
	}
}

